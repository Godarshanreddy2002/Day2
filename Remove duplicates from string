class Solution {
    public String removeDuplicateLetters(String s) {
           StringBuilder sb=new StringBuilder();
          Set<Character> aL=new HashSet<>();
           for(int i=0;i<s.length();i++){                       
                aL.add(s.charAt(i));                
           }           
           for(Character ch:aL){
           sb.append(ch);            
           }
           return sb.toString();
    }
}
